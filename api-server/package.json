{
  "name": "qed-api-server",
  "version": "1.0.0",
  "description": "API Gateway for QED Labyrinth",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "start": "node dist/index.js",
    "proto:gen": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./src/schemas_generated -I=../schemas ../schemas/input.proto ../schemas/output.proto"
  },
  "author": "Tenebrous Labs",
  "dependencies": {
    "@fastify/jwt": "^8.0.1",
    "@fastify/websocket": "^10.0.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "pg": "^8.12.0",
    "redis": "^4.6.15"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.14.9",
    "@types/pg": "^8.11.6",
    "@types/ws": "^8.18.1",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^5.5.3"
  }
}
